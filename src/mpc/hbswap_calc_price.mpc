def read_sint(idx):
    start = regint(idx * 32)
    end = regint()
    masked_value = sint()
    readsharesfromfile(start, end, masked_value)
    return masked_value

def sint_to_sfix(x):
    return sfix._new(x)

def main():
    total_price = sint_to_sfix(read_sint(0))

    total_cnt = read_sint(1)

    avg_price = total_price / total_cnt
    print_ln('%s', avg_price.reveal())

main()
